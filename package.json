{
  "name": "eoapi-tapdata",
  "version": "1.0.0",
  "description": "Import the JSON format file of the OpenAPI specification",
  "main": "dist/index.js",
  "moduleID": "eoapi-import-tapdata",
  "moduleName": "Import Tapdata",
  "moduleType": "feature",
  "author": "Tapdata",
  "logo": "https://avatars.githubusercontent.com/u/36888935?s=200&v=4",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js"
  },
  "devDependencies": {
    "esbuild": "^0.14.38",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1"
  },
  "features": {
    "apimanage.import": {
      "action": "importFunc",
      "label": "OpenAPI(.json)",
      "description": "${function.description}",
      "icon": "https://avatars.githubusercontent.com/u/36888935?s=200&v=4"
    },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  }
}
